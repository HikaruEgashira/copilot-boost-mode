{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react",
    "jsxFactory": "vscpp",
    "jsxFragmentFactory": "vscppf",
    "lib": [
      "esnext"
    ],
    "module": "CommonJS",
    "outDir": "out",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "ESNext",
    "useUnknownInCatchVariables": false
  },
  "exclude": [
    "node_modules",
    ".vscode-test"
  ],
  "include": [
    "src",
    "tests/e2e",
    "vscode.proposed.chatProvider.d.ts"
  ]
}
